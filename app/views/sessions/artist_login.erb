<div class="wrapper">
<h2>Artist Login</h2>
<%= form_tag(login_artist_path) do %>
 <%= label_tag :email %>
 <%= text_field_tag :email %>
<br>
 <%= label_tag :password %>
 <%= password_field_tag :password %>
<br>
 <%= submit_tag 'Login', id:"login" %>
 <br>
<% end %>
<p>
Are you an artist?
<%= link_to  "Sign Up Here" , new_artist_path %>
</p>

  <h3>
<%= link_to "Customer Login", '/login' %></h3>
</div>
