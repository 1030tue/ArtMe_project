
<h2>Client Login</h2>
<!-- form will POST to ‘/login’ -->
<%= form_tag(login_path) do %>
 <%= label_tag :email %>
 <%= text_field_tag :email %>
<br>
 <%= label_tag :password %>
 <%= password_field_tag :password %>
<br>
 <%= submit_tag 'Login' %>
 <br>
 <p>
Need Art? <%= link_to "Join Us", new_client_path %></p>
<% end %>

<%= link_to "Artist login", 'login/artist' %>
